                        "type": "object",
                        "properties": {
                            "fullName": {
                                "$ref": "../types.json#/components/schemas/FullName"
                            },
                            "email": {
                                "$ref": "../types.json#/components/schemas/Email"
                            },
                            "phone": {
                                "$ref": "../types.json#/components/schemas/Phone"
                            },
                            "ssn": {
                                "type": "string",
                                "nullable": true,
                                "pattern": "^\\d{9}|\\d{4}"
                            },
                            "passport": {
                                "type": "string",
                                "nullable": true
                            },
                            "nationality": {
                                "$ref": "../types.json#/components/schemas/Country",
                                "nullable": true
                            },
                            "matriculaConsular": {
                                "type": "string",
                                "nullable": true
                            },
                            "address": {
                                "$ref": "../types.json#/components/schemas/Address"
                            },
                            "dateOfBirth": {
                                "type": "string",
                                "format": "date"
                            },
                            "dba": {
                                "type": "string",
                                "nullable": true
                            },
                            "ein": {
                                "type": "string",
                                "nullable": true,
                                "pattern": "^\\d{9}$"
                            },
                            "ip": {
                                "type": "string"
                            },
                            "soleProprietorship": {
                                "type": "boolean",
                                "default": false
                            },
                            "tags": {
                                "$ref": "../types.json#/components/schemas/Tags"
