{
    "components": {
        "schemas": {
            "AtmLocation": {
                "title": "ATM Location",
                "type": "object",
                "properties": {
                    "type": {
                        "type": "string",
                        "enum": ["atmLocation"],
                        "default": "atmLocation"
                    },
                    "attributes": {
                        "type": "object",
                        "properties": {
                            "network": {
                                "type": "string",
                                "description": "The ATM network."
                            },
                            "locationName": {
                                "type": "string",
                                "description": "The name of the ATM location."
                            },
                            "coordinates": {
                                "allOf": [
                                    { "$ref": "../types.json#/components/schemas/Coordinates" },
                                    { "description": "The coordinates of the ATM location." }
                                ]
                            },
                            "address": {
                                "allOf": [
                                    { "$ref": "../types.json#/components/schemas/Address" },
                                    { "description": "The address of the ATM location." }
                                ]
                            },
                            "distance": {
                                "type": "number",
                                "description": "The distance from the search location in miles."
                            },
                            "surchargeFree": {
                                "type": "boolean",
                                "description": "Indicates whether the ATM is surcharge-free."
                            },
                            "acceptDeposits": {
                                "type": "boolean",
                                "description": "Indicates whether the ATM accepts deposits."
                            }
                        }
                    }
                },
                "required": ["type", "attributes"],
                "additionalProperties": false
            }
        }
    }
}
