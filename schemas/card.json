            "physicalCardStatus": {
                "type": "string",
                "enum": [
                    "Active",
                    "Inactive",
                    "Stolen",
                    "Lost",
                    "Frozen",
                    "ClosedByCustomer",
                    "Unknown",
                    "SuspectedFraud",
                    "ActiveForOnlineUse"
                ]
            },
            "virtualCardStatus": {
                "type": "string",
                "enum": [
                    "Active",
                    "Inactive",
                    "Stolen",
                    "Lost",
                    "Frozen",
                    "ClosedByCustomer",
                    "Unknown",
                    "SuspectedFraud"
                ]
            },
