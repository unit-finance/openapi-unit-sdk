{
    "components": {
        "schemas": {
            "PhysicalCardStatus": {
                "type": "string",
                "enum": [
                    "Active",
                    "Inactive",
                    "Stolen",
                    "Lost",
                    "Frozen",
                    "ClosedByCustomer",
                    "Unknown",
                    "SuspectedFraud",
                    "ActiveForOnlineUse"
                ]
            },
            "VirtualCardStatus": {
                "type": "string",
                "enum": [
                    "Active",
                    "Inactive",
                    "Stolen",
                    "Lost",
                    "Frozen",
                    "ClosedByCustomer",
                    "Unknown",
                    "SuspectedFraud"
                ]
            },
            "CardRelationships": {
                "type": "object",
                "title": "Card Relationships",
                "properties": {
                    "allOf": [
                        {
                            "ref": "../relationships.json$/components/schemas/AccountRelationship"
                        },
                        {
                            "ref": "../relationships.json$/components/schemas/CustomerRelationship"
                        }
                    ]
                },
                "required": ["account"],
                "additionalProperties": false
            },
            "PinStatus": {
                "title": "Pin Status",
                "type": "object",
                "properties": {
                    "type": {
                        "type": "string",
                        "default": "pinStatus"
                    },
                    "attributes": {
                        "type": "object",
                        "properties": {
                            "status": {
                                "type": "string",
                                "enum": ["Set", "NotSet"]
                            }
                        }
                    }
                }
            },
            "IndividualDebitCard": {
                "title": "Individual Debit Card",
                "type": "object",
                "allOf": [
                    {
                        "$ref": "#/components/schemas/Card"
                    },
                    {
                        "type": "object",
                        "properties": {
                            "attributes": {
                                "type": "object",
                                "properties": {
                                    "createdAt": {
                                        "type": "string",
                                        "format": "date-time"
                                    },
                                    "updatedAt": {
                                        "type": "string",
                                        "format": "date-time"
                                    },
                                    "last4Digits": {
                                        "type": "string",
                                        "pattern": "^\\d{4}$"
                                    },
                                    "expirationDate": {
                                        "type": "string",
                                        "pattern": "^\\d{4}[-](1[012]|0[1-9])$"
                                    },
                                    "nextExpirationDate": {
                                        "type": "string",
                                        "pattern": "^\\d{4}[-](1[012]|0[1-9])$"
                                    },
                                    "shippingAddress": {
                                        "$ref": "../types.json#/components/schemas/Address"
                                    },
                                    "bin": {
                                        "type": "string",
                                        "pattern": "^\\d{9}$"
                                    },
                                    "status": {
                                        "$ref": "#/components/schemas/PhysicalCardStatus"
                                    },
                                    "design": {
                                        "type": "string"
                                    },
                                    "tags": {
                                        "$ref": "../types.json#/components/schemas/Tags"
                                    },
                                    "freezeReason": {
                                        "type": "string"
                                    }
                                },
                                "required": [
                                    "createdAt",
                                    "last4Digits",
                                    "expirationDate",
                                    "status"
                                ],
                                "additionalProperties": false
                            },
                            "relationships": {
                                "$ref": "#/components/schemas/CardRelationships"
                            }
                        },
                        "additionalProperties": false,
                        "required": ["attributes", "relationships"]
                    }
                ]
            },
