								"type":"object",
								"properties":{
									"data":{
										"type":"array",
										"items":{
											"$ref":"card.json"
										}
									},
									"included":{
										"type":"array",
										"items":{
											"type":"object"
										}
									}
								}
							}
						}
					}
				}
			}
		},
