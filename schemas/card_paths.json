								"title":"UnitCardResponseCardsList",
								"type":"object",
								"properties":{
									"data":{
										"type":"array",
										"items":{
											"$ref":"./card.json#/components/schemas/card"
										}
									},
									"included":{
										"type":"array",
										"items":{
											"$ref": "./openapi_types.json#/IncludedResource"
										}
