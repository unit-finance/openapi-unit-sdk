{
    "components": {
        "schemas": {
            "checkDepositStatus": {
                "type": "string",
                "enum": [
                    "AwaitingImages",
                    "AwaitingFrontImage",
                    "AwaitingBackImage",
                    "AwaitingCustomerConfirmation",
                    "Pending",
                    "PendingReview",
                    "Rejected",
                    "Clearing",
                    "Sent",
                    "Canceled",
                    "Returned"
                ]
            },
            "checkDepositRelationships": {
                "type": "object",
                "title": "checkDepositRelationships",
                "properties": {
                    "org": {
                        "$ref": "./types.json#/components/schemas/orgRelationship"
                    },
                    "account": {
                        "type": "object",
                        "properties": {
                            "data": {
                                "type": "object",
                                "properties": {
                                    "type": {
                                        "type": "string",
                                        "enum": [
                                            "account",
                                            "depositAccount",
                                            "batchAccount"
                                        ]
                                    },
                                    "id": {
                                        "$ref": "./types.json#/components/schemas/identifier"
                                    }
                                },
                                "required": ["type", "id"],
                                "additionalProperties": false
                            }
                        },
                        "required": ["data"],
                        "additionalProperties": true
                    },
                    "customer": {
                        "type": "object",
                        "properties": {
                            "data": {
                                "type": "object",
                                "properties": {
                                    "type": {
                                        "type": "string",
                                        "enum": [
                                            "businessCustomer",
                                            "individualCustomer",
                                            "customer"
                                        ]
                                    },
                                    "id": {
                                        "$ref": "./types.json#/components/schemas/identifier"
                                    }
                                },
                                "required": ["type", "id"],
                                "additionalProperties": false
                            }
                        },
                        "required": ["data"],
                        "additionalProperties": false
                    },
                    "transaction": {
                        "type": "object",
                        "properties": {
                            "data": {
                                "type": "object",
                                "properties": {
                                    "type": {
                                        "type": "string",
                                        "default": "transaction"
                                    },
                                    "id": {
                                        "$ref": "./types.json#/components/schemas/identifier"
                                    }
                                },
                                "required": ["type", "id"],
                                "additionalProperties": false
                            }
                        },
                        "required": ["data"],
                        "additionalProperties": false
                    }
                },
                "required": ["account"],
                "additionalProperties": false
            },
            "statusEvent": {
                "type": "object",
                "properties": {
                    "updatedAt": {
                        "type": "string"
                    },
                    "updatedBy": {
                        "$ref": "./types.json#/components/schemas/identifier"
                    },
                    "status": {
                        "type": "object",
                        "properties": {
                            "type": {
                                "$ref": "#/components/schemas/checkDepositStatus"
                            }
                        }
                    }
                },
                "required": ["status"]
            },
            "counterparty": {
                "title": "Counterparty",
                "type": "object",
                "properties": {
                    "routingNumber": {
                        "type": "string",
                        "pattern": "^\\d{9}$"
                    },
                    "accountNumber": {
                        "type": "string",
                        "maxLength": 17
                    },
                    "name": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 250
                    }
                },
                "additionalProperties": false,
                "required": ["routingNumber", "accountNumber", "name"]
            },
            "checkDeposit": {
                "title": "Check Deposit",
                "type": "object",
                "properties": {
                    "type": {
                        "type": "string",
                        "enum": ["checkDeposit"]
                    },
                    "id": {
                        "$ref": "./types.json#/components/schemas/identifier"
                    },
                    "attributes": {
                        "type": "object",
                        "properties": {
                            "createdAt": {
                                "type": "string",
                                "format": "date-time"
                            },
                            "settlementDate": {
                                "type": "string",
                                "format": "date"
                            },
                            "amount": {
                                "type": "integer",
                                "minimum": 1
                            },
                            "description": {
                                "type": "string",
                                "minLength": 1,
                                "maxLength": 50
                            },
                            "status": {
                                "type": "string",
                                "enum": [
                                    "AwaitingImages",
                                    "AwaitingFrontImage",
                                    "AwaitingBackImage",
                                    "Pending",
                                    "PendingReview",
                                    "AwaitingCustomerConfirmation",
                                    "Rejected",
                                    "Clearing",
                                    "Sent",
                                    "Canceled",
                                    "Returned"
                                ]
                            },
                            "reason": {
                                "type": ["string", "null"]
                            },
                            "statusCreatedAt": {
                                "type": "string",
                                "format": "date-time"
                            },
                            "statusSetBy": {
                                "type": "string"
                            },
                            "statusHistory": {
                                "type": "array",
                                "items": {
                                    "$ref": "#/components/schemas/statusEvent"
                                }
                            },
                            "reasonText": {
                                "type": "string"
                            },
                            "checkNumber": {
                                "type": "string"
                            },
                            "vendor": {
                                "type": "string"
                            },
                            "counterparty": {
                                "$ref": "./counterparty.json#/components/schemas/counterparty"
                            },
                            "tags": {
                                "$ref": "tags.json#/components/schemas/tags"
                            }
                        },
                        "required": [
                            "createdAt",
                            "amount",
                            "description",
                            "status"
                        ],
                        "additionalProperties": false
                    },
                    "relationships": {
                        "$ref": "#/components/schemas/checkDepositRelationships"
                    }
                },
                "additionalProperties": false,
                "required": ["type", "id", "attributes", "relationships"]
            }
        }
    }
}
