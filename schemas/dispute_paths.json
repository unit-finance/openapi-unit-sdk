{
    "components": {
        "schemas": {
            "disputeRelationships": {
                "type": "object",
                "title": "disputeRelationships",
                "properties": {
                    "customer": {
                        "$ref": "types.json#/definitions/Relationship"
                    },
                    "account": {
                        "$ref": "types.json#/definitions/Relationship"
                    },
                    "transaction": {
                        "$ref": "types.json#/definitions/Relationship"
                    }
                },
                "required": [
                    "customer",
                    "account",
                    "transaction"
                ],
                "additionalProperties": false
            },
            "Dispute": {
                "title": "Dispute Document",
                "type": "object",
                "properties": {
                    "type": {
                        "type": "string",
                        "default": "dispute"
                    },
                    "id": {
                        "$ref": "types.json#/definitions/identifier"
                    },
                    "attributes": {
                        "type": "object",
                        "properties": {
                            "source": {
                                "type": "string",
                                "enum": [
                                    "DebitCard",
                                    "ACH"
                                ]
                            },
                            "externalId": {
                                "type": "string",
                                "maxLength": 255
                            },
                            "link": {
                                "$ref": "types.json#/definitions/url"
                            },
                            "description": {
                                "$ref": "types.json#/definitions/description"
                            },
                            "amount": {
                                "type": "integer"
                            },
                            "status": {
                                "type": "string"
                            },
                            "statusHistory": {
                                "type": "array",
                                "items": {
                                    "type": "object",
                                    "properties": {
                                        "type": {
                                            "type": "string"
                                        },
                                        "updatedAt": {
                                            "type": "string",
                                            "format": "date-time"
                                        }
                                    },
                                    "additionalProperties": false
                                }
                            },
                            "createdAt": {
                                "type": "string",
                                "format": "date-time"
                            },
                            "decisionReason": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "externalId",
                            "source",
                            "description",
                            "status",
                            "createdAt",
                            "amount"
                        ],
                        "additionalProperties": false
                    },
                    "relationships": {
                        "$ref": "#/components/schemas/disputeRelationships"
                    }
                },
                "additionalProperties": false,
                "required": [
                    "type",
                    "id",
                    "relationships"
                ]
            }
        }
    },
    "list_disputes": {
        "get": {
            "tags": [
                "Get List Disputes"
            ],
            "operationId": "execute",
            "summary": "Get List Disputes",
            "description": "Get List Disputes from API ",
            "parameters": [
                {
                    "name": "page",
                    "in": "query",
                    "schema": {
                        "$ref": "types.json#/definitions/listPageParametersObject"
                    }
                },
                {
                    "name": "filter",
                    "in": "query",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "query": {
                                "type": "string"
                            },
                            "status": {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "enum": [
                                        "Pending",
                                        "Approved"
                                    ]
                                }
                            },
                            "email": {
                                "type": "string"
                            },
                            "tags": {
                                "type": "object"
                            }
                        }
                    }
                },
                {
                    "name": "sort",
                    "in": "query",
                    "schema": {
                        "type": "string",
                        "enum": [
                            "createdAt",
                            "-createdAt"
                        ],
                        "default": "-createdAt"
                    }
                }
            ],
            "responses": {
                "200": {
                    "description": "Successful Response",
                    "content": {
                        "application/vnd.api+json; charset=utf-8": {
                            "schema": {
                                "type": "array",
                                "title": "UnitDisputesListResponse",
                                "items": {
                                    "$ref": "#/components/schemas/Dispute"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "get_dispute": {
        "get": {
            "tags": [
                "Get Dispute"
            ],
            "operationId": "execute",
            "parameters": [
                {
                    "schema": {
                        "type": "string"
                    },
                    "name": "disputeId",
                    "in": "path",
                    "required": true,
                    "description": "ID of the dispute to get"
                }
            ],
            "summary": "Get Dispute by Id",
            "description": "Get a Dispute from API ",
            "responses": {
                "200": {
                    "description": "Successful Response",
                    "content": {
                        "application/vnd.api+json; charset=utf-8": {
                            "schema": {
                                "type": "object",
                                "title": "UnitDisputeResponse",
                                "properties": {
                                    "data": {
                                        "$ref": "#/components/schemas/Dispute"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}