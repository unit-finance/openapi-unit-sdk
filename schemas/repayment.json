{
    "components": {
        "schemas": {
            "RepaymentRelationships": {
                "type": "object",
                "properties": {
                    "creditAccount": {
                        "$ref": "./types.json#/components/schemas/Relationship"
                    },
                    "org": {
                        "$ref": "./types.json#/components/schemas/Relationship"
                    },
                    "payment": {
                        "$ref": "./types.json#/components/schemas/Relationship"
                    },
                    "customer": {
                        "$ref": "./types.json#/components/schemas/Relationship"
                    },
                    "recurringRepayment": {
                        "$ref": "./types.json#/components/schemas/Relationship"
                    }
                }
            },
            "BaseRepayment": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "type": {
                        "type": "string"
                    },
                    "attributes": {
                        "type": "object",
                        "properties": {
                            "createdAt": {
                                "type": "string"
                            },
                            "updatedAt": {
                                "type": "string"
                            },
                            "amount": {
                                "type": "number"
                            },
                            "tags": {
                                "$ref": "tags.json#/components/schemas/tags"
                            }
                        },
                        "required": ["createdAt", "updatedAt", "amount"]
                    },
                    "relationships": {
                        "$ref": "#/components/schemas/RepaymentRelationships"
                    }
                },
                "required": ["id", "type", "attributes", "relationships"]
            },
            "BookRepaymentStatus": {
                "type": "string",
                "enum": ["Sent", "Rejected"]
            },
            "BaseBookRepayment": {
                "allOf": [
                    {
                        "$ref": "#/components/schemas/BaseRepayment"
                    },
                    {
                        "type": "object",
                        "properties": {
                            "attributes": {
                                "type": "object",
                                "properties": {
                                    "status": {
                                        "$ref": "#/components/schemas/BookRepaymentStatus"
                                    }
                                },
                                "required": ["status"]
                            },
                            "relationships": {
                                "type": "object",
                                "properties": {
                                    "counterpartyAccount": {
                                        "$ref": "./types.json#/components/schemas/Relationship"
                                    }
                                },
                                "required": ["counterpartyAccount"]
                            }
                        },
                        "required": ["attributes", "relationships"]
                    }
                ]
            },
            "BookRepayment": {
                "allOf": [
                    {
                        "$ref": "#/components/schemas/BaseBookRepayment"
                    },
                    {
                        "type": "object",
                        "properties": {
                            "type": {
                                "type": "string",
                                "enum": ["bookRepayment"]
                            },
                            "relationships": {
                                "type": "object",
                                "properties": {
                                    "account": {
                                        "$ref": "./types.json#/components/schemas/Relationship"
                                    }
                                },
                                "required": ["account"]
                            }
                        },
                        "required": ["type", "relationships"]
                    }
                ]
            },
            "CapitalPartnerBookRepayment": {
                "allOf": [
                    {
                        "$ref": "#/components/schemas/BaseBookRepayment"
                    },
                    {
                        "type": "object",
                        "properties": {
                            "type": {
                                "type": "string",
                                "enum": ["capitalPartnerBookRepayment"]
                            }
                        },
                        "required": ["type"]
                    }
                ]
            },
            "AchRepaymentStatus": {
                "type": "string",
                "enum": [
                    "Pending",
                    "PendingReview",
                    "Clearing",
                    "Returned",
                    "Sent",
                    "Rejected",
                    "Canceled"
                ]
            },
            "BaseAchRepayment": {
                "allOf": [
                    {
                        "$ref": "#/components/schemas/BaseRepayment"
                    },
                    {
                        "type": "object",
                        "properties": {
                            "attributes": {
                                "type": "object",
                                "properties": {
                                    "status": {
                                        "$ref": "#/components/schemas/AchRepaymentStatus"
                                    }
                                },
                                "required": ["status"]
                            },
                            "relationships": {
                                "type": "object",
                                "properties": {
                                    "counterparty": {
                                        "$ref": "./types.json#/components/schemas/Relationship"
                                    }
                                },
                                "required": ["counterparty"]
                            }
                        },
                        "required": ["attributes", "relationships"]
                    }
                ]
            },
            "AchRepayment": {
                "allOf": [
                    {
                        "$ref": "#/components/schemas/BaseAchRepayment"
                    },
                    {
                        "type": "object",
                        "properties": {
                            "type": {
                                "type": "string",
                                "enum": ["achRepayment"]
                            },
                            "relationships": {
                                "type": "object",
                                "properties": {
                                    "account": {
                                        "$ref": "./types.json#/components/schemas/Relationship"
                                    }
                                },
                                "required": ["account"]
                            }
                        },
                        "required": ["type", "relationships"]
                    }
                ]
            },
            "CapitalPartnerAchRepayment": {
                "allOf": [
                    {
                        "$ref": "#/components/schemas/BaseAchRepayment"
                    },
                    {
                        "type": "object",
                        "properties": {
                            "type": {
                                "type": "string",
                                "enum": ["capitalPartnerAchRepayment"]
                            }
                        },
                        "required": ["type"]
                    }
                ]
            },
            "repayment": {
                "title": "Repayment Resource",
                "type": "object",
                "properties": {
                    "type": {
                        "type": "string"
                    },
                    "id": {
                        "$ref": "./types.json#/components/schemas/identifier"
                    }
                },
                "discriminator": {
                    "propertyName": "type",
                    "mapping": {
                        "achRepayment": "#/components/schemas/AchRepayment",
                        "bookRepayment": "#/components/schemas/BookRepayment",
                        "capitalPartnerAchRepayment": "#/components/schemas/CapitalPartnerAchRepayment",
                        "capitalPartnerBookRepayment": "#/components/schemas/CapitalPartnerBookRepayment"
                    }
                }
            }
        }
    }
}
