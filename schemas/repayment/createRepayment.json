                        "type": "object",
                        "properties": {
                            "description": {
                                "type": "string"
                            },
                            "amount": {
                                "type": "number"
                            },
                            "addenda": {
                                "type": "string"
                            },
                            "sameDay": {
                                "type": "boolean"
                            },
                            "secCode": {
                                "type": "string"
                            }
                        },
                        "required": ["description", "amount"],
                        "additionalProperties": false
