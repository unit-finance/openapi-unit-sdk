                "type": "object",
                "properties": {
                    "data": {
                        "oneOf": [
                            {
                                "$ref": "#/components/schemas/CreateBookRepayment"
                            },
                            {
                                "$ref": "#/components/schemas/CreateCapitalPartnerBookRepayment"
                            },
                            {
                                "$ref": "#/components/schemas/CreateAchRepayment"
                            },
                            {
                                "$ref": "#/components/schemas/CreateCapitalPartnerAchRepayment"
                            }
