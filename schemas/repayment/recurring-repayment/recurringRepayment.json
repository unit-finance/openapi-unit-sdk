                                    "type": {
                                        "type": "string",
                                        "default": "creditAccount"
                                    },
                                    "id": {
                                        "$ref": "../../types.json#/components/schemas/Identifier"
                                    }
                                },
                                "required": ["type", "id"],
                                "additionalProperties": false
                            }
                        },
                        "required": ["data"],
                        "additionalProperties": false
                    },
                    "counterparty": {
                        "type": "object",
