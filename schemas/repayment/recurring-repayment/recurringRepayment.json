                            "data": {
                                "type": "object",
                                "properties": {
                                    "type": {
                                        "type": "string",
                                        "enum": ["counterparty"]
                                    },
                                    "id": {
                                        "$ref": "../../types.json#/components/schemas/Identifier"
                                    }
                                },
