{
	"definitions":{
		"identifier":{
			"type":"string",
			"pattern":"^[1-9]\\d*$"
		},
		"email":{
			"type":"string",
			"format":"email"
		},
		"state":{
			"type":"string",
			"pattern":"^((A[LKSZR])|(C[AOT])|(D[EC])|(F[ML])|(G[AU])|(HI)|(I[DLNA])|(K[SY])|(LA)|(M[EHDAINSOT])|(N[EVHJMYCD])|(MP)|(O[HKR])|(P[WAR])|(RI)|(S[CD])|(T[NX])|(UT)|(V[TIA])|(W[AVIY]))$"
		},
		"caState":{
			"type":"string",
			"pattern":"^(AB|BC|MB|N[BLTSU]|ON|PE|QC|SK|YT)$"
		},
		"usAddress":{
			"title":"Address",
			"type":"object",
			"properties":{
				"street":{
					"type":"string",
					"minLength":1,
					"maxLength":255,
					"pattern":"^.*$"
				},
				"street2":{
					"type":[
						"string",
						"null"
					],
					"pattern":"^.*$"
				},
				"city":{
					"type":"string",
					"minLength":1,
					"maxLength":255,
					"pattern":"^.*$"
				},
				"state":{
					"$ref":"#/definitions/state"
				},
				"postalCode":{
					"type":"string",
					"pattern":"^[0-9]{5}(?:-[0-9]{4})?$"
				},
				"country":{
					"type":"string",
					"const":"US"
				}
			},
			"additionalProperties":false,
			"required":[
				"street",
				"city",
				"postalCode",
				"country",
				"state"
			]
		},
		"caAddress":{
			"title":"Address",
			"type":"object",
			"properties":{
				"street":{
					"type":"string",
					"minLength":1,
					"maxLength":255,
					"pattern":"^.*$"
				},
				"street2":{
					"type":[
						"string",
						"null"
					],
					"pattern":"^.*$"
				},
				"city":{
					"type":"string",
					"minLength":1,
					"maxLength":255,
					"pattern":"^.*$"
				},
				"state":{
					"$ref":"#/definitions/caState"
				},
				"postalCode":{
					"type":"string",
					"pattern":"^(?!.*[DFIOQU])[A-VXY][0-9][A-Z] ?[0-9][A-Z][0-9]$"
				},
				"country":{
					"type":"string",
					"const":"CA"
				}
			},
			"additionalProperties":false,
			"required":[
				"street",
				"city",
				"postalCode",
				"country",
				"state"
			]
		},
		"internationalAddress":{
			"title":"Address",
			"type":"object",
			"properties":{
				"street":{
					"type":"string",
					"minLength":1,
					"maxLength":255,
					"pattern":"^.*$"
				},
				"street2":{
					"type":[
						"string",
						"null"
					],
					"pattern":"^.*$"
				},
				"city":{
					"type":"string",
					"minLength":1,
					"maxLength":255,
					"pattern":"^.*$"
				},
				"state":{
					"oneOf":[
						{
							"type":"null"
						},
						{
							"type":"string",
							"maxLength":255,
							"pattern":"^.*$"
						}
					]
				},
				"postalCode":{
					"type":"string",
					"minLength":1,
					"maxLength":20
				},
				"country":{
					"$ref":"#/definitions/countryNoUSCA"
				}
			},
			"additionalProperties":false,
			"required":[
				"street",
				"city",
				"postalCode",
				"country"
			]
		},
		"address":{
			"oneOf":[
				{
					"$ref":"#/definitions/usAddress"
				},
				{
					"$ref":"#/definitions/caAddress"
				},
				{
					"$ref":"#/definitions/internationalAddress"
				}
			]
		},
		"jwtSubject":{
			"type":[
				"string",
				"null"
			]
		},
		"contact":{
			"title":"Contact",
			"type":"object",
			"properties":{
				"fullName":{
					"$ref":"#/definitions/fullName"
				},
				"email":{
					"$ref":"types.json#/definitions/email"
				},
				"phone":{
					"$ref":"#/definitions/phone"
				},
				"jwtSubject":{
					"$ref":"#/definitions/jwtSubject"
				}
			},
			"additionalProperties":false
		},
		"responseContact":{
			"allOf":[
				{
					"$ref":"#/definitions/contact"
				}
			],
			"required":[
				"fullName"
			]
		},
		"createContact":{
			"allOf":[
				{
					"$ref":"#/definitions/contact"
				}
			],
			"required":[
				"fullName",
				"email",
				"phone"
			]
		},
		"prefillContact":{
			"allOf":[
				{
					"$ref":"#/definitions/contact"
				}
			]
		},
		"trustContact":{
			"title":"Contact",
			"type":"object",
			"properties":{
				"fullName":{
					"$ref":"#/definitions/fullName"
				},
				"email":{
					"$ref":"types.json#/definitions/email"
				},
				"phone":{
					"$ref":"#/definitions/phone"
				},
				"address":{
					"$ref":"#/definitions/address"
				},
				"jwtSubject":{
					"$ref":"#/definitions/jwtSubject"
				}
			},
			"additionalProperties":false
		},
		"trustee":{
			"title":"Trustee",
			"type":"object",
			"properties":{
				"fullName":{
					"$ref":"types.json#/definitions/fullName"
				},
				"email":{
					"$ref":"types.json#/definitions/email"
				},
				"phone":{
					"$ref":"types.json#/definitions/phone"
				},
				"ssn":{
					"type":"string",
					"pattern":"^\\d{9}$"
				},
				"passport":{
					"type":"string"
				},
				"nationality":{
					"$ref":"types.json#/definitions/country"
				},
				"matriculaConsular":{
					"type":"string"
				},
				"address":{
					"$ref":"types.json#/definitions/address"
				},
				"dateOfBirth":{
					"type":"string",
					"format":"date"
				}
			},
			"oneOf":[
				{
					"properties":{
						"ssn":{
							"type":"string",
							"pattern":"^\\d{9}$"
						}
					},
					"required":[
						"ssn"
					]
				},
				{
					"properties":{
						"passport":{
							"type":"string"
						},
						"nationality":{
							"$ref":"types.json#/definitions/country"
						}
					},
					"required":[
						"passport",
						"nationality"
					]
				},
				{
					"properties":{
						"matriculaConsular":{
							"type":"string"
						}
					},
					"required":[
						"matriculaConsular"
					]
				}
			],
			"additionalProperties":false,
			"required":[
				"fullName",
				"email",
				"phone",
				"address",
				"dateOfBirth"
			]
		},
		"grantor":{
			"title":"Grantor",
			"type":"object",
			"properties":{
				"fullName":{
					"$ref":"types.json#/definitions/fullName"
				},
				"email":{
					"$ref":"types.json#/definitions/email"
				},
				"phone":{
					"$ref":"types.json#/definitions/phone"
				},
				"ssn":{
					"type":"string",
					"pattern":"^\\d{9}$"
				},
				"passport":{
					"type":"string"
				},
				"matriculaConsular":{
					"type":"string"
				},
				"nationality":{
					"$ref":"types.json#/definitions/country"
				},
				"address":{
					"$ref":"types.json#/definitions/address"
				},
				"dateOfBirth":{
					"type":"string",
					"format":"date"
				},
				"evaluationId":{
					"type":"string"
				},
				"evaluationFlags":{
					"type":"array",
					"items":{
						"type":"string"
					}
				},
				"status":{
					"type":"string"
				},
				"maskedSSN":{
					"type":"string",
					"pattern":"^\\*{5}\\d{4}$"
				}
			},
			"oneOf":[
				{
					"properties":{
						"ssn":{
							"type":"string",
							"pattern":"^\\d{9}$"
						}
					},
					"required":[
						"ssn"
					]
				},
				{
					"properties":{
						"passport":{
							"type":"string"
						},
						"nationality":{
							"$ref":"types.json#/definitions/country"
						}
					},
					"required":[
						"passport",
						"nationality"
					]
				},
				{
					"properties":{
						"matriculaConsular":{
							"type":"string"
						}
					},
					"required":[
						"matriculaConsular"
					]
				}
			],
			"additionalProperties":false,
			"required":[
				"fullName",
				"email",
				"phone",
				"address",
				"dateOfBirth"
			]
		},
		"beneficiary":{
			"title":"Beneficiary",
			"type":"object",
			"properties":{
