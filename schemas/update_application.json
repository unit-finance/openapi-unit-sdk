                                "deviceFingerprints": {
                                    "type": "array",
                                    "maxItems": 1,
                                    "items": {
                                        "$ref": "types.json#/definitions/deviceFingerprint"
                                    }
                                },
                                "jwtSubject": {
                                    "$ref": "types.json#/definitions/jwtSubject"
                                },
