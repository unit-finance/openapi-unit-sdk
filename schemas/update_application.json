                "title": "Patch Business Application",
                "type": "object",
                "properties": {
                    "type": {
                        "type": "string",
                        "default": "businessApplication"
                    },
                    "attributes": {
                        "type": "object",
                        "properties": {
                            "tags": {
                                "$ref": "tags.json#/components/schemas/tagsPatch"
